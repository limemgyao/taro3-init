{
  "name": "taro3",
  "version": "1.0.0",
  "private": true,
  "description": "我的项目",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "cross-env BUILD_ENV=development npx taro build --type weapp",
    "build:weapp:production": "cross-env BUILD_ENV=production npx taro build --type weapp",
    "build:h5": "taro build --type h5",
    "build:h5:production": "cross-env BUILD_ENV=production npx taro build --type h5",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:weapp:production": "BUILD_ENV=production npm run build:weapp:production -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:h5:production": "BUILD_ENV=production npm run build:h5 -- --watch",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@opcjs/zoro": "^3.0.5",
    "@opcjs/zoro-plugin": "^3.0.0",
    "@tarojs/cli": "3.3.16",
    "@tarojs/components": "3.3.16",
    "@tarojs/plugin-less": "^2.2.10",
    "@tarojs/plugin-sass": "^2.2.10",
    "@tarojs/plugin-uglify": "^2.2.10",
    "@tarojs/react": "3.3.16",
    "@tarojs/runtime": "3.3.16",
    "@tarojs/taro": "3.3.16",
    "async-validator": "^4.0.7",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "dotenv-flow": "^3.2.0",
    "lodash": "^4.17.15",
    "qs": "^6.10.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.6",
    "taro-ui": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.3.16",
    "@tarojs/webpack-runner": "3.3.16",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.3.16",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.3.16",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "stylelint": "9.3.0",
    "typescript": "^4.1.0"
  }
